<h2 class="user_new_box_title">New medication</h2>
<%= form_for Medication.new do |f| %>
<%= f.text_field :description, :placeholder => 'Description' %>
<%= f.text_field :name, :placeholder => 'Name' %>
<%= f.text_field :dose, :placeholder => 'Dose' %>
<%= f.submit :create, :value => "Save" %>
<% end %>